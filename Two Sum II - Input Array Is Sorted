class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int p = nums.size();
        for (int i = 0; i < p; i++){
            for (int j  = i+1; j < p; j++){
                if (nums[i] + nums[j] == target){
                    return {i, j};
                }
            }
        }
        return {};
    }
};
